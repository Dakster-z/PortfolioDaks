# Environment variables for the frontend (Create React App)
# NOTE: Only expose non-sensitive, public keys in the frontend.
# For any secret keys, use a backend server.

# EmailJS (public keys only)
REACT_APP_EMAILJS_SERVICE_ID=your_service_id
REACT_APP_EMAILJS_TEMPLATE_ID=your_template_id
REACT_APP_EMAILJS_PUBLIC_KEY=your_public_key

# Analytics (example)
REACT_APP_ANALYTICS_WRITE_KEY=your_analytics_key

# API Base URL (example)
REACT_APP_API_BASE_URL=https://api.example.com